    {
      "type": "timeseries",
      "title": "Postęp analizy (kumulatywny)",
      "description": "Kumulatywna liczba repozytoriów discovered, fetched i analyzed w czasie",
      "gridPos": { "h": 9, "w": 14, "x": 0, "y": 6 },
      "id": 7,
      "datasource": { "type": "postgres", "uid": "PA942B37CCFAF5A81" },
      "targets": [
        {
          "datasource": { "type": "postgres", "uid": "PA942B37CCFAF5A81" },
          "rawQuery": true,
          "rawSql": "SELECT discovered_time AS time, SUM(COUNT(*)) OVER (ORDER BY discovered_time) AS \"Discovered\" FROM repositories_repository GROUP BY discovered_time ORDER BY discovered_time",
          "refId": "A",
          "format": "time_series"
        },
        {
          "datasource": { "type": "postgres", "uid": "PA942B37CCFAF5A81" },
          "rawQuery": true,
          "rawSql": "SELECT fetch_time AS time, SUM(COUNT(*)) OVER (ORDER BY fetch_time) AS \"Fetched\" FROM repositories_repository WHERE fetch_time IS NOT NULL GROUP BY fetch_time ORDER BY fetch_time",
          "refId": "B",
          "format": "time_series"
        },
        {
          "datasource": { "type": "postgres", "uid": "PA942B37CCFAF5A81" },
          "rawQuery": true,
          "rawSql": "SELECT analyzed_time AS time, SUM(COUNT(*)) OVER (ORDER BY analyzed_time) AS \"Analyzed\" FROM repositories_repository WHERE analyzed = true AND analyzed_time IS NOT NULL GROUP BY analyzed_time ORDER BY analyzed_time",
          "refId": "C",
          "format": "time_series"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "spanNulls": true,
            "lineInterpolation": "linear",
            "showPoints": "auto"
          },
          "unit": "short"
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Discovered" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "blue" } }]
          },
          {
            "matcher": { "id": "byName", "options": "Fetched" },
            "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "orange" } }]
